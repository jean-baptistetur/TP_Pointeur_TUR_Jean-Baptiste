#include <stdio.h>

typedef struct {
    const char* option;
} Menu;

typedef struct {
    const char* nom;
    Menu* menus;
    int nb_menus;
} Application;

int main() {
    Menu testMenu = { "Option de test" };
    Menu menus[] = { testMenu };
    Application testApp = { "Application de test", menus, 1 };

    printf("Nom de l'application : %s\n", testApp.nom);
    printf("Option du menu : %s\n", testApp.menus[0].option);

    return 0;
}
